{
  "name": "yuko",
  "version": "0.2.0-dev",
  "description": "A Discord Library Written In JavaScript",
  "main": "index.js",
  "engines": {
    "node": ">=14.0.0"
  },
  "scripts": {
    "docs": "docgen --source lib --custom docs/index.yml --output docs/docs.json",
    "docs:test": "docgen --source lib --custom docs/index.yml",
    "lint": "eslint -c .eslintrc.yml lib examples *.js",
    "lint:fix": "eslint -c .eslintrc.yml lib examples *.js --fix && echo \"\u001b[1m\u001b[32mOK\u001b[39m\u001b[22m\" || echo \"\u001b[1m\u001b[31mNot OK\u001b[39m\u001b[22m\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NotMarx/yuko.git"
  },
  "keywords": [
    "yuko",
    "discord",
    "discord-api",
    "discord-library",
    "javascript",
    "js"
  ],
  "author": "reinhardt",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/NotMarx/yuko/issues"
  },
  "homepage": "https://github.com/NotMarx/yuko#readme",
  "devDependencies": {
    "@discordjs/docgen": "github:discordjs/discord.js-docgen",
    "@types/node": "^16.4.13",
    "@types/ws": "^7.4.7",
    "@typescript-eslint/eslint-plugin": "^4.29.1",
    "@typescript-eslint/parser": "^4.29.1",
    "eslint": "^7.32.0",
    "eslint-plugin-sort-class-members": "^1.11.0",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "node-fetch": "^2.6.1",
    "ws": "^7.4.4"
  }
}
